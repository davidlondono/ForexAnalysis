target 'ForexAnalisis' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'SwiftLint'
    
  # Pods for ForexAnalisis
  pod 'RxSwift',    '~> 4.0'
  pod 'RxCocoa',    '~> 4.0'
  pod 'RxRealm'
  pod 'RxSwiftExt'
  pod 'RxKeyboard'
  pod 'RxViewModel'
  

  target 'ForexAnalisisTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'ForexAnalisisUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RxBlocking', '~> 4.0'
    pod 'RxTest',     '~> 4.0'
  end

end
